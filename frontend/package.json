{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gcreate": "clasp create --type webapp --title 'ATT Dashboard' --rootDir ../backend",
    "gclone": "clasp clone --rootDir ./src",
    "gpull": "clasp pull",
    "gpush": "clasp push",
    "gstart": "clasp push --watch",
    "start": "parcel ./src/index.html --dist-dir ../local --no-hmr",
    "build": "parcel build ./src/index.html --no-cache --no-source-maps --dist-dir  ../backend",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@google/clasp": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "sass": "^1.78.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "parcel": "^2.12.0",
    "vite": "^5.4.1"
  }
}
